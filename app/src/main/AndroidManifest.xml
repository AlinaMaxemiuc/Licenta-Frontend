<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logonobg"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/logonobg"
        android:supportsRtl="true"
        android:theme="@style/Theme.Renting"
        tools:targetApi="31">
        <activity
            android:name=".SignUp"
            android:exported="false"
            android:theme="@style/Theme.Renting.LoginPage"/>
        <activity
            android:name=".ProfileActivity"
            android:exported="false"
            android:theme="@style/Theme.Renting.LoginPage"/>
        <activity
            android:name=".RateActivity"
            android:exported="false"
            android:theme="@style/Theme.Renting.LoginPage"/>
        <activity
            android:name=".MyRentalsActivity"
            android:exported="false"
            android:theme="@style/Theme.Renting.LoginPage"/>
        <activity
            android:name=".LoginPage"
            android:exported="false"
            android:theme="@style/Theme.Renting.LoginPage" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".RentPage"
            android:exported="false"
            android:theme="@style/Theme.Renting.RentPage"/>
        <activity
            android:name=".FirstPage"
            android:exported="true"
            android:theme="@style/Base.Theme.Renting">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>